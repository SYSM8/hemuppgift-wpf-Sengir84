<Window x:Class="Hemuppgift_WPFtemplate.DockPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hemuppgift_WPFtemplate"
        mc:Ignorable="d"
        Title="DockPanel" Height="450" Width="800">
    <DockPanel>
        
        <Button Content="Top" DockPanel.Dock="Top" Height="30" Width="80" Margin="5" HorizontalAlignment="Center"/>
        <Button Content="Bottom" DockPanel.Dock="Bottom" Height="30" Width="80" Margin="5" VerticalAlignment="Center"/>
        <Button Content="Left" DockPanel.Dock="Left" Height="30" Width="80" Margin="5"/>
        <Button Content="Right" DockPanel.Dock="Right" Height="30" Width="80" Margin="5"/>
            <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
                <Button x:Name="BtnCenter" Content="Center" Height="30" Width="80" Margin="5" VerticalAlignment="Center" HorizontalAlignment="Center" Click="BtnCenter_Click"/>
                <Image x:Name="trophy" Visibility="Collapsed" Width="200" Height="200" VerticalAlignment="Center" HorizontalAlignment="Center"/>
            </Grid>
    </DockPanel>
   
</Window>
